<?xml version="1.0"?>
<!DOCTYPE benchmark PUBLIC "+//IDN sosy-lab.org//DTD BenchExec benchmark 1.4//EN" "http://www.sosy-lab.org/benchexec/benchmark-1.4.dtd">
<benchmark tool="ultimateautomizer" timelimit="90 s" hardtimelimit="96 s" memlimit="7 GB" cpuCores="2">

<require cpuModel="Intel Core i7-6700" cpuCores="2"/>

  <resultfiles>**.graphml</resultfiles>
  
  <rundefinition name="sv-comp17-violation-witness">
    <requiredfiles>../results-verified/LOGDIR/sv-comp17.${inputfile_name}.files/witness.graphml</requiredfiles> 
  </rundefinition>

  <option name="../../results-verified/LOGDIR/sv-comp17.${inputfile_name}.files/witness.graphml"/>

  <tasks name="ArraysReach">
    <exclude>../sv-benchmarks/c/*/*_true-unreach-call*</exclude>
    <includesfile>../sv-benchmarks/c/ArraysReach.set</includesfile>
    <propertyfile>../sv-benchmarks/c/ArraysReach.prp</propertyfile>
    <option name="32bit"/>
    <option name="precise"/>
  </tasks>
  <tasks name="ArraysMemSafety">
    <exclude>../sv-benchmarks/c/*/*_true-valid-memsafety*</exclude>
    <includesfile>../sv-benchmarks/c/ArraysMemSafety.set</includesfile>
    <propertyfile>../sv-benchmarks/c/ArraysMemSafety.prp</propertyfile>
    <option name="32bit"/>
  </tasks>
  <tasks name="BitVectorsReach">
    <exclude>../sv-benchmarks/c/*/*_true-unreach-call*</exclude>
    <includesfile>../sv-benchmarks/c/BitVectorsReach.set</includesfile>
    <propertyfile>../sv-benchmarks/c/BitVectorsReach.prp</propertyfile>
    <option name="32bit"/>
  </tasks>
  <tasks name="BitVectorsOverflows">
    <exclude>../sv-benchmarks/c/*/*_true-no-overflow*</exclude>
    <includesfile>../sv-benchmarks/c/BitVectorsOverflows.set</includesfile>
    <propertyfile>../sv-benchmarks/c/BitVectorsOverflows.prp</propertyfile>
    <option name="64bit"/>
  </tasks>
  <tasks name="HeapReach">
    <exclude>../sv-benchmarks/c/*/*_true-unreach-call*</exclude>
    <includesfile>../sv-benchmarks/c/HeapReach.set</includesfile>
    <propertyfile>../sv-benchmarks/c/HeapReach.prp</propertyfile>
    <option name="32bit"/>
  </tasks>
  <tasks name="HeapMemSafety">
    <exclude>../sv-benchmarks/c/*/*_true-valid-memsafety*</exclude>
    <includesfile>../sv-benchmarks/c/HeapMemSafety.set</includesfile>
    <propertyfile>../sv-benchmarks/c/HeapMemSafety.prp</propertyfile>
    <option name="32bit"/>
