  </tasks>
  <tasks name="Termination-Main">
    <exclude>../sv-benchmarks/c/*/*_true-termination*</exclude>
    <includesfile>../sv-benchmarks/c/Termination-Main.set</includesfile>
    <propertyfile>../sv-benchmarks/c/Termination.prp</propertyfile>
