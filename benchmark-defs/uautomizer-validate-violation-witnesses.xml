    <option name="32bit"/>
  </tasks>
  <tasks name="Loops">
    <exclude>../sv-benchmarks/c/*/*_true-unreach-call*</exclude>
    <includesfile>../sv-benchmarks/c/Loops.set</includesfile>
    <propertyfile>../sv-benchmarks/c/Loops.prp</propertyfile>
    <option name="32bit"/>
  </tasks>
  <tasks name="Recursive">
