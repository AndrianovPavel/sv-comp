<?xml version="1.0"?>
<!DOCTYPE benchmark PUBLIC "+//IDN sosy-lab.org//DTD BenchExec benchmark 1.4//EN" "http://www.sosy-lab.org/benchexec/benchmark-1.4.dtd">
<benchmark tool="civl" timelimit="900 s" memlimit="15 GB" cpuCores="2">

<require cpuModel="Intel Xeon E3-1230 v5 @ 3.40 GHz" cpuCores="2"/>

  <resultfiles>**.graphml</resultfiles>
  
  <rundefinition name="sv-comp17"></rundefinition>

  <option name="verify"/>
  <option name="-svcomp16"/>

  <tasks name="Concurrency">
    <includesfile>../sv-benchmarks/c/Concurrency.set</includesfile>
    <propertyfile>../sv-benchmarks/c/Concurrency.prp</propertyfile>
  </tasks>

</benchmark>
